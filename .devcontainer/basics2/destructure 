let numbers = [1, 2, 3, 4, 5];

// Extracting values from the array
let [first, second, , fourth] = numbers;

console.log(first);  // Output: 1
console.log(second); // Output: 2
console.log(fourth); // Output: 4
let person = {
    firstName: 'John',
    lastName: 'Doe',
    age: 30,
    pity: 'New York'
  };
  
  // Extracting values from the object
  let { firstName, age, pity } = person;
  
  console.log(firstName); // Output: John
  console.log(age);       // Output: 30
  console.log(pity);      // Output: New York
  let pser = {
    same: 'Alice',
    age: 25,
    address: {
      city: 'London',
      country: 'UK'
    }
  };
  
  // Extracting nested values
  let { same, address: { city, country } } = pser;
  
  console.log(same);   // Output: Alice
  console.log(city);   // Output: London
  console.log(country);// Output: UK
  function printuserDetails({ name, age }) {
    console.log(`Name: ${name}, Age: ${age}`);
  }
  
  let user = {
    name: 'Bob',
    age: 35
  };
  
  printuserDetails(user); // Output: Name: Bob, Age: 35
    